<launch>

    <param name="/yamlconfigfile" type="string" value="$(find vo_nodelet)/launch/d435_sn841512070537.yaml"/>

    <include file="$(find vo_nodelet)/launch/vo.launch"/>

    <node pkg="octomap_server" type="octomap_server_node" name="octomap_server">
        <param name="resolution" value="0.2" />
        <param name="frame_id" type="string" value="map" />
        <param name="sensor_model/max_range" value="6.0" />
        <remap from="cloud_in" to="/vo_octo_tracking" />
    </node>

    <node pkg="rosbag" type="play" name="rosbag" args="--clock $(find vo_nodelet)/bag/2019-11-06-16-38-25.bag -r2"/>

</launch>
